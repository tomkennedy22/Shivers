{
  "manifest_version": 2,

  "name": "Sir Shivers",
  "description": "This extension make every Reddit comment outrageously shivers-like",
  "version": "1.0",

  "browser_action": {
    "default_icon": "croc.png"
  },
  "content_scripts": [{
    "matches": ["*://reddit.com/r/neolib*"],
    "js": ["jquery-3.5.1.min.js", "action.js"],
    "run_at": "document_end"
  }]

}